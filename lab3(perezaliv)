#include <iostream>

void PrintM(int num)
{
	int ir = 0, nr = 0, kr = 0, jr = 0, k =1, j=1, n=1, numr=0;
	int a[1000] = {};
	numr = num;
	numr = numr * (-1);
	if (num > 0)
	{
		for (int i = 2; i <= (num / 2); i++)
		{
			ir = i;
			if ((num % i) == 0)
			{
				a[ir - 2] = i;
			}
		}
		while (k < (num / 2))
		{
			while (j < (num / 2))
			{
				while (n < (num / 2))
				{
					if (((a[n] * a[j] * a[k]) == num) and (n != k) and (k != j) and (n != j))
					{
						nr = n;
						kr = k;
						jr = j;
					}
					n = n + 1;
				}
				j = j + 1;
				n = 0;
			}
			k = k + 1;
			j = 0;
		}
		std::cout << a[nr] << " " << a[kr] << " " << a[jr] << std::endl;
	}
	else
	{
			for (int i = 2; i <= (numr / 2); i++)
			{
				ir = i;
				if ((numr % i) == 0)
				{
					a[ir - 2] = i;
				}
			}
			while (k < (numr / 2))
			{
				while (j < (numr / 2))
				{
					while (n < (numr / 2))
					{
						if (((a[n] * a[j] * a[k]) == numr) and (n != k) and (k != j) and (n != j))
						{
							nr = n;
							kr = k;
							jr = j;
						}
						n = n + 1;
					}
					j = j + 1;
					n = 0;
				}
				k = k + 1;
				j = 0;
			}
		std::cout << a[nr] << " " << (a[kr] * (-1)) << " " << a[jr] << std::endl;
	}
}
using namespace std;
int main()
{
	int Inum = 3;
	cout << "Enter number" << endl;
	cin >> Inum;
	PrintM(Inum);
	cout << "\nOperation succeeded\n";
	return 0;
}
